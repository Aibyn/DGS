type Query {
    products(type: ProductType): [Product]
}

type Mutation {
    addProduct(product: InputProduct): [Product]
}

type Product {
    name: String
    price: Int
    type: ProductType
}

input InputProduct {
    name: String
    price: Int
    type: ProductType
}

enum ProductType {
    WEIGHT
    PACKAGE
    SINGLE
}